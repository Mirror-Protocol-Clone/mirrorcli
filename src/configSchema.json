            }
          },
          "title": "lcd"
        },
        "contracts": {
          "type": "object",
          "properties": {
            "collector": {
              "type": "string",
              "title": "collector"
            },
            "factory": {
              "type": "string",
              "title": "factory"
            },
            "gov": {
              "type": "string",
              "title": "gov"
            },
            "mint": {
              "type": "string",
              "title": "mint"
            },
            "oracle": {
              "type": "string",
              "title": "oracle"
            },
            "staking": {
              "type": "string",
              "title": "staking"
            },
            "mirror-token": {
              "type": "string",
              "title": "mirror-token"
            },
            "ts-factory": {
              "type": "string",
              "title": "ts-factory"
            },
            "ts-pair": {
              "type": "string",
              "title": "ts-pair"
            }
          },
          "title": "contracts"
        },
        "assets": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/AssetConfig"
          },
          "title": "assets"
        }
      }
    }
  }
