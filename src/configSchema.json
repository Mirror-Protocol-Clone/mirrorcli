{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/MirrorCLIConfig",
  "definitions": {
    "AssetConfig": {
      "title": "AssetConfig",
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "title": "name"
        },
        "token": {
          "type": "string",
          "title": "token"
        },
        "pair": {
          "type": "string",
          "title": "pair"
        },
        "lpToken": {
          "type": "string",
          "title": "lpToken"
        }
      },
      "required": [
        "lpToken",
        "name",
        "pair",
        "token"
      ]
    },
    "MirrorCLIConfig": {
      "title": "MirrorCLIConfig",
      "type": "object",
      "properties": {
        "lcd": {
          "type": "object",
          "properties": {
            "chainId": {
              "type": "string",
