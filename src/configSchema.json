            },
            "url": {
              "type": "string",
              "title": "url"
            },
            "gasPrices": {
              "type": "object",
              "title": "gasPrices",
              "properties": {
                "uusd": {
                  "type": "number"
                },
                "umnt": {
                  "type": "number"
                },
                "usdr": {
                  "type": "number"
                },
                "uluna": {
                  "type": "number"
                },
                "ukrw": {
                  "type": "number"
                }
              },
              "additionalProperties": false
